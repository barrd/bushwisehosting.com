/* BWH Screen CSS
--------------------------------------------------------------------*/

body {
  background: #fedc7b url(../imgs/shared/bgMainContent.jpg) no-repeat fixed center 0;
  margin: 0; 
  text-align: left;
  font-size: 12px;
  color: #a74300;
}
body.error { 
  background: #fff url(../imgs/shared/bgMainError.jpg) no-repeat;
  margin: 20px 10% 0 10%;
  text-align: justify;
  line-height: 18px;
}

/* Shared Main Content IDs (& Wrappers)
--------------------------------------------------------------------*/

#totalWrapper {
  width: 1000px;
  margin: 0 auto;
}
#headerWrapper {
  width: 980px;
  height: 125px;
  padding: 0 10px 0 10px;
}
#headerLhWrapper {
  float: left;
  width: 740px;
  color: #fafafa;
}
#headerRhWrapper {
  float: right;
  width: 240px;
  text-align: right;
  font-size: 10px;
}
#headLinksWrapper {
  float: left;
  width: 980px;
  height: 19px;
  padding: 4px 10px 4px 10px;
  background-color: #a74300;
  border-top: 1px solid #fcc25f;
  color: #fcc25f;
}
#headLinks210, #headLinks190, #headLinks200L, #headLinks200R, #headLinks180 {
  float: left;
  position: relative;
  padding-top: 2px;
}
#headLinks210 { /* Logo */
  width: 210px;
}
#headLinks190 { /* Contact */
  width: 190px;
}
#headLinks200L { /* Hosting */
  width: 200px;
}
#headLinks200R { /* News */
  width: 200px;
}
#headLinks180 { /* Register & Login|out */
  float: right;
  position: relative;
  width: 180px;
  text-align: right;
  padding-top: 2px;
}
#menuLhNavWrapper { /* Left hand Links */
  float: left;
  width: 190px;
  padding: 0 0 0 10px;
  margin-top: 8px;
  margin-bottom: 20px;
}
#contentMainWrapper { /* Main Page Content */
  float: right;
  text-align: justify;
  line-height: 18px;
  width: 770px;
  margin-top: 0;
  padding: 0 10px 0 0;
}
#eopWrapper { /* Footer */
  font-size: 9px;
  line-height: 14px;
  display: block;
  padding-top: 24px;
}
/* Left Hand Navigation & Related Rh Table
--------------------------------------------------------------------*/
#relatedWrapper {
  float: right;
  position: relative;
  background-color: #fcd895;
  width: 200px;
  margin: 0 0 4px 12px;
  border-left: 1px solid #f93;
  border-right: 1px solid #f93;
}
.menuHeadNav, .menuHeadRel { 
  display: block;
  width: auto;
  padding: 4px 0 2px 4px;
  border-bottom: 2px solid #f93;
}
.menuHeadRel {
  background-color: #f9bc72;
}
.menuContent, .menuRelContent, .menuContentCurrPos {
  display: block; 
  padding: 6px 0 6px 4px; 
  line-height: normal;
}
.menuContent, .menuContentCurrPos {
  width: 186px;
  border-bottom: 1px dotted #f93;
}
.menuContentCurrPos {
  background-color: #ffff85;
  font-weight: bold;
  color: #630;
}
.menuRelContent {
  width: 196px;
  border-bottom: 1px solid #f93;
}
.menuRelContent:hover, .menuContent:hover {
  background-color: #ffff85;
}
/* shoutBox* creates CSS box for highlighting context of paragraph 
--------------------------------------------------------------------*/
.shoutBoxL, .shoutBoxR {
  margin: 0;
  width: 125px;
  text-align: center;
  font-size: 18px;
  line-height: 24px;
  color: #c96;
}
.shoutBoxL {
  float: left;
  padding: 0 4px 4px 0;
}
.shoutBoxR {
  float: right;
  padding: 0 0 4px 4px;
}
/* infoTb* creates CSS table with 2 headers & single content body
--------------------------------------------------------------------*/
.infoTbWrapper {
  width: 100%;
  border: 1px solid #630;
  margin-bottom: 12px;
}
.infoTbL, .infoTbR, .infoTbBody {
  padding: 6px;
}
.infoTbL, .infoTbR {
  height: 18px;
  border-bottom: 1px solid #630;
}
.infoTbL {
  float: left;
  width: 617px;
  background-color: #fae067;
  border-right: 1px dotted #630;
}
.infoTbR {
  float: right;
  position: relative;
  width: 128px;
  text-align: right;
  background-color: #fcc25f;
}
.infoTbBody {
  clear: both;
  width: 758px;
  background-color: #f7c55c;
  padding-top: 12px;
  text-align: justify;
  line-height: 18px;
}
/* halfTb* creates CSS table with 2 content cells 50% each
--------------------------------------------------------------------*/
.halfTbL {
  float: left;
  width: 50%;
}
.halfTbR {
  float: right;
  position: relative;
  width: 50%;
}
/* Text and Image classes
--------------------------------------------------------------------*/
.lightTxt {
  color: #fafafa;
}
.darkTxt {
  color: #a74300;
}
.noScreen { /* Hide content from Screen UAs */
  display: none;
}
.req { /* Required form fields */
  border-bottom: 1px dotted #1262c7;
  color: #1262c7;
}
.smTxt {
  font-size: 9px;
  line-height: 12px;
  text-align: left;
}
.aRight { /* Align elements right */
  float: right; 
  text-align: right; 
}
.imgTxt { /* Imgs next to text */
  border: 0; 
  vertical-align: baseline;
  margin-right: 1px;
}
.uc {
  text-transform: uppercase;
}
/* Text and Image links
--------------------------------------------------------------------*/
a.imgLink:link, a.imgLink:active, a.imgLink:visited, a.imgLink:hover,
a.head:link, a.head:active, a.head:visited, a.head:hover,
a.menu:link, a.menu:active, a.menu:visited, a.menu:hover {
  text-decoration: none;
}
a:link, a:active, a:visited {
  text-decoration: underline;
  color: #f63;
}
a:hover { 
  background-color: #fae274;
  border: 0;
  color: #630;
}
a.imgLink:link, a.imgLink:active, a.imgLink:visited, a.imgLink:hover {
  background-color: transparent;
  border: 0;
}
a.head:link, a.head:active, a.head:visited {
  background-color: transparent;
  border: 0;
  outline: none;
  color: #ff3;
}
a.head:hover {
  background-color: transparent;
  border: 0;
  color: #fcc25f;
}
a.menu:link, a.menu:active, a.menu:visited {
  color: #f63;
}
a.menu:hover {
  border: 0;
  color: #630;
}
a.noBg:hover { /* 'a name' TOP */
  background-color: transparent;
  color: #fafafa;
}
/* Typography
--------------------------------------------------------------------*/
body, button, select, textarea, .txtInput, td, th {
  font-family: Arial, Helvetica, sans-serif;
}
code, pre {
  font-family: 'Andale Mono', 'Courier New', Courier, monospace;
  font-size: 100%;
}
body.error, #headerRhWrapper, .shoutBoxL, .shoutBoxR, .un  {
  font-family: 'Comic Sans MS', Arial, Helvetica, sans-serif;
}
#headerRhWrapper, #headLinksWrapper, .shoutBoxL, .shoutBoxR, .infoTbL, .infoTbR, a, dt, legend, label, th {
  font-weight: bold;
}
.hh1, .hh2 {
  margin: 0;
  color: #fafafa;
}
.hh1 {
  font-size: 24px;
}
.hh2 {
  font-size: 18px;
}
h1 {
  font-size: 20px;
  margin: 14px 0 -2px 0;
  color: #930;
}
h2, h3, h4 {
  color: #835548;
}
h2 {
  font-size: 16px;
  margin: 9px 0 0 0;
  line-height: 26px;
}
h3 {
  font-size: 14px;
  margin: 0;
  line-height: 20px;
}
h4 {
  font-size: 12px;
  margin: 0;
}
p { 
  margin: 4px 0 12px 0;
}
br {
  clear: both;
}
acronym {
  text-decoration: none;
  border: none;
  letter-spacing: 1px;
  color: #960;
}
acronym:hover {
  cursor: help;
}
code {
  color: #66c;
}
code.code { 
  color: #a74300;
}
pre {
  background-color: #fdcf6c;
  padding: 6px;
  border: 1px dotted #a74300;
  color: #630;
}
pre.example {
  margin: 0;
  padding: 0;
}
em {
  font-style: normal;
  color: #630;
}
strong { 
  padding: 2px;
  line-height: 18px;
  border: 1px solid #36c;
  color: #1262c7;
}
sup {
  margin: -6px 0 0 0;
  font-size: 8px;
}
/* "other" elements
--------------------------------------------------------------------*/
img {
  border: 0 !important;
}
hr { 
  border: 1px dotted #f93;
  border-width: 1px 0 0 0;
}
/* Lists
--------------------------------------------------------------------*/
ol, ul {
  line-height: 18px;
}
ol.lcR {
  list-style-type: lower-roman;
}
ul { 
  list-style-image: url(../imgs/shared/iconDisk.png);
}
ul.star { 
  list-style-image: url(../imgs/shared/iconStar.png);
}
dt {
  color: #630;
  border-bottom: 1px dotted #f93;
}
dd {
  margin: 6px 0 6px 36px;
  padding: 0 0 6px 0;
}
/* Forms
--------------------------------------------------------------------*/
select, textarea, textarea.sm, .txtInput, option {
  font-size: 12px;
}
fieldset {
  background: url(../imgs/shared/bgClearWhite.png);
  margin: 12px 0 0 0;
  border: 1px solid #630;
  padding: 12px 0 6px 6px;
  line-height: 18px;
  text-align: justify;
}
fieldset.sm {
  clear: both;
  margin: 0 0 12px 0;
  width: 350px;
  padding: 8px 0 0 6px;
}
legend {
  background: #fc0;
  border: 1px solid #630;
  padding: 2px 10px 2px 10px;
}
label {
  float: left;
  width: 100px;
  text-align: right;
  margin: 0 6px 0 0;
  white-space: pre;
}
optgroup {
  font-size: 100%;
  font-style: normal;
  background-color: #ddd;
  padding: 1px 0 0 0;
  border-top: 1px solid #666;
  color: #444;
}
option, optgroup.option {
  background-color: #fafafa;
  padding: 2px 0 2px 12px;
  color: #a74300;
}
select, textarea, .txtInput {
  float: left;
  line-height: 14px;
  background-color: #fafafa;
  border: 1px solid #a74300;
  padding: 2px;
  margin: 0 6px 20px 0;
  color: #a74300;
}
textarea {
  width: 75%;
  height: 96px;
}
textarea.sm {
  width: 60%;
  height: 48px;
}
:focus { /* Field focus */
  outline: 1px dotted invert;
}
button {
  font-size: 100%;
  background-color: #f63;
  border: 2px outset #f90;
  padding: 1px 6px;
  margin: 6px 20px 12px 10px;
  color: #ff3;
}
button:hover {
  outline: 1px solid #630;
} 
button:active {
  border: 2px inset #f90;
}
select, button, label {
  cursor: pointer;
}
/* Tables (--border-collapse: separate;)
--------------------------------------------------------------------*/
table {
  border-collapse: collapse;
  margin: 0;
  border: 0;
}
table.lg {
  width: 100%;
  margin: 6px 0;
}
th, td {
  border: 1px solid #630;
  padding: 6px;
  font-size: 100%;
  line-height: normal;
}
th {
  background-color: #b77a43;
  text-align: left;
  color: #630;
}
tr.rowOdd {
  background-color: #ffff85;
}
tr.rowEven {
  background-color: #fdcf6c;
}
